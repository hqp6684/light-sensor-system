{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es5",
    "lib": ["es2015"],
    "module": "commonjs",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    // "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "allowJs": true,
    "typeRoots": ["./node_modules/@types"]
  }
}
